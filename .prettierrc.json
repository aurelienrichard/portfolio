{
	"arrowParens": "always",
	"printWidth": 100,
	"semi": false,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "none",
	"useTabs": true,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"tailwindConfig": "tailwind.config.cjs",
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
