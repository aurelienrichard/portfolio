{
	"name": "privelte",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "vite build",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"clean": "rm -rf .turbo && rm -rf coverage",
		"dev": "vite dev",
		"lint": "prettier . --check . && eslint . --report-unused-disable-directives",
		"preview": "vite preview",
		"test:coverage": "vitest --coverage",
		"test:ui": "vitest --ui",
		"test:watch": "vitest",
		"test": "vitest run"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^2.6.1",
		"@skeletonlabs/tw-plugin": "^0.3.0",
		"@sveltejs/adapter-cloudflare": "^3.0.1",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@testing-library/jest-dom": "^6.1.6",
		"@testing-library/svelte": "^4.0.5",
		"autoprefixer": "^10.4.16",
		"eslint-config-custom": "workspace:*",
		"happy-dom": "^12.10.3",
		"postcss": "^8.4.32",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tailwind-config": "workspace:*",
		"tailwindcss": "^3.3.6",
		"tsconfig": "workspace:*",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.10",
		"vitest": "^1.1.1"
	},
	"type": "module"
}
