{
	"name": "privelte",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "vite build",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"clean": "rm -rf .turbo && rm -rf coverage",
		"dev": "vite dev",
		"lint": "prettier . --check . && eslint . --report-unused-disable-directives",
		"preview": "vite preview",
		"test:coverage": "vitest --coverage",
		"test:ui": "vitest --ui",
		"test:watch": "vitest",
		"test": "vitest run",
		"update-types": "pnpx supabase gen types typescript --project-id yipoupsiomqlwjtapqda --schema public > src/lib/types/supabase.ts"
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@repo/tailwind-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@skeletonlabs/skeleton": "^2.9.0",
		"@skeletonlabs/tw-plugin": "^0.3.1",
		"@sveltejs/adapter-cloudflare": "^4.2.0",
		"@sveltejs/kit": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/forms": "^0.5.7",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/svelte": "^4.1.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/node": "^20.11.28",
		"@vitest/coverage-v8": "^1.4.0",
		"@vitest/ui": "^1.4.0",
		"autoprefixer": "^10.4.18",
		"happy-dom": "^13.8.6",
		"postcss": "^8.4.35",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.7",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.6",
		"vite-plugin-tailwind-purgecss": "^0.2.0",
		"vitest": "^1.4.0",
		"wrangler": "^3.34.2"
	},
	"type": "module",
	"dependencies": {
		"@highlight-run/cloudflare": "^2.1.5",
		"@supabase/supabase-js": "^2.39.8",
		"highlight.run": "^8.8.0",
		"nanoid": "^5.0.6",
		"openpgp": "^5.11.1",
		"unique-names-generator": "^4.7.1",
		"zod": "^3.22.4"
	}
