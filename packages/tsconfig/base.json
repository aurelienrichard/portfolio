{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Default",
	"compilerOptions": {
		"target": "ESNext",
		"composite": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"preserveWatchOutput": true,
		"skipLibCheck": true,
		"strict": true
	},
	"exclude": ["node_modules"]
}
